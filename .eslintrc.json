{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "eslint:recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-console": "off",
        "no-unused-vars": ["error", { "vars": "all", "ignoreRestSiblings": false }],
        "quotes": [
            "error",
            "double"
          ],
        "linebreak-style": 0
    }
}