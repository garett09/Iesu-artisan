        <section class="testimonial" id="testimonial">
          <div class="container">
            <div class="title">
              <h2 class="wow animate__animated animate__bounceIn animate__slow">
                Feedbacks
                <br>
                <br>
                
              </h2>
              <table id="example" class="table-bordered table-hover table-condensed" style="width:100%">
             
                <thead>
                  <tr>
                    <th scope="col" class="th-sm">ID</th>
                    <th scope="col" class="th-sm">How will you rate the products you received?</th>
                    <th scope="col" class="th-sm">How satisfied are you with our products?</th>
                    <th scope="col" class="th-sm">How satisfied are you with the service provided?</th>
                   
                  </tr>
                </thead>
                <tbody>
                  <% for(var i = 0; i < user.length; i++) { %>
                    <tr>
                        <td><%= i + 1 %></td>
                        <td><%= user[i].feedBack1 %></td>
                        <td><%= user[i].feedBack2 %></td>
                        <td><%= user[i].feedBack3 %></td>

                    </tr>
                    <% } %>
                </tbody>
                <tfoot>
                  <tr>
                    <th scope="col">ID</th>
                    <th scope="col">How will you rate the products you received?</th>
                    <th scope="col">How satisfied are you with our products?</th>
                    <th scope="col">How satisfied are you with the service provided?</th>
                   
                  </tr>
                </tfoot>
              </table>


              
            </div>
          
